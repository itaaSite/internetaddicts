{
  "version": 2,
  "collections": [
    {
      "id": "default",
      "name": "Default",
      "postsPath": "src/content/posts",
      "imagesPath": "public",
      "template": {
        "fields": [
          {
            "name": "title",
            "type": "string"
          },
          {
            "name": "description",
            "type": "string"
          },
          {
            "name": "pubDate",
            "type": "date"
          },
          {
            "name": "img",
            "type": "string"
          },
          {
            "name": "tags",
            "type": "array"
          }
        ]
      },
      "tableColumns": [
        "description",
        "pubDate",
        "img",
        "tags"
      ],
      "columnWidths": {
        "__name__": 35
      }
    }
  ],
  "activeCollectionId": "default",
  "settings": {
    "projectType": "astro",
    "domainUrl": "https://internetaddicts.ru",
    "postFileTypes": ".md,.mdx",
    "imageFileTypes": "image/*",
    "publishDateSource": "file",
    "imageCompressionEnabled": false,
    "maxImageSize": 500,
    "imageResizeMaxWidth": 1024
  },
  "commitMessages": {
    "newPost": "feat(content): add post \"{filename}\"",
    "updatePost": "fix(content): update post \"{filename}\"",
    "newImage": "feat(assets): add image \"{filename}\"",
    "updateImage": "refactor(assets): update image for \"{filename}\""
  }
}